[core]
	editor = vim
	excludesfile = /Users/khendrick/.gitignore_global
[user]
	name = Kirk Hendrick
	email = khendrick@acumensolutions.com
[credential]
	helper = osxkeychain
[difftool "sourcetree"]
	cmd = /Applications/kdiff3.app/Contents/MacOS/kdiff3 \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/kdiff3.app/Contents/MacOS/kdiff3 \"$BASE\" \"$LOCAL\" \"$REMOTE\" -o \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/khendrick/.stCommitMsg
